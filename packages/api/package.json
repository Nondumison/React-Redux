{
  "name": "sample-api",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^12",
    "yarn": "^1.17"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "cross-env-shell NODE_ENV=production 'sequelize db:migrate && sequelize db:seed:all && node -r esm lib/index.js'",
    "start:dev": "cross-env-shell NODE_ENV=development 'sequelize db:migrate && sequelize db:seed:all && nodemon --inspect -r esm -r @babel/register src/'",
    "build": "rimraf lib && cross-env NODE_ENV=production babel src -d lib",
    "test": "cross-env NODE_ENV=test jest --env=node --watch",
    "test:nowatch": "cross-env NODE_ENV=test jest --env=node",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "release": "standard-version",
    "cz": "git-cz",
    "lint": "cross-env NODE_ENV=development eslint src",
    "flow-typed-install": "flow-typed install --skip --ignoreDeps dev peer bundled",
    "flow-typed-createstub": "flow-typed create-stub --overwrite",
    "lint-staged": "lint-staged",
    "flow-coverage-report": "flow-coverage-report; true",
    "precommit": "npm-run-all lint-staged flow-coverage-report"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@casl/ability": "^3.1.2",
    "@feathersjs/authentication": "^2.1.5",
    "@feathersjs/authentication-jwt": "^2.0.0",
    "@feathersjs/authentication-local": "^1.1.2",
    "@feathersjs/configuration": "^2.0.6",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.3",
    "@feathersjs/feathers": "^3.1.7",
    "@feathersjs/socketio": "^3.2.2",
    "@hapi/joi": "^15.1.0",
    "babel-plugin-console": "^0.2.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-react-app": "^9.0.0",
    "bluebird": "^3.5.1",
    "change-case": "^3.1.0",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "dauria": "^2.0.0",
    "dotenv-flow": "^3.0.0",
    "esm": "^3.2.25",
    "feathers-blob": "^2.1.0",
    "feathers-hooks-common": "^4.14.3",
    "feathers-sequelize": "^6.0.0",
    "flow-typed": "^2.5.1",
    "fs-blob-store": "^5.2.1",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "multer": "^1.3.1",
    "node-fetch": "^2.2.0",
    "nodemailer": "^6.3.0",
    "nodemailer-express-handlebars": "^3.0.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "pg": "^7.4.3",
    "qs": "^6.8.0",
    "query-types": "^0.1.4",
    "rimraf": "^2.6.3",
    "sequelize": "^5.10.1",
    "sequelize-cli": "^5.5.0",
    "uuid": "^3.3.3",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-babel": "^5.1",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-module-resolver": "^0.13.0",
    "eslint-plugin-no-use-extend-native": "^0.4.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-unicorn": "^8.0.2",
    "flow-bin": "^0.105.2",
    "flow-coverage-report": "^0.6.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "nodemon": "^1.18.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "standard-version": "^7.0.1"
  }
}
